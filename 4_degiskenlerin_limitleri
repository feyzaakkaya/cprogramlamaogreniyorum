#include <stdio.h>
#include <limits.h>

int main() {

    printf("SIGNED CHAR icin alabilecegi min. deger = %d\n", SCHAR_MIN);
    printf("SIGNED CHAR icin alabilecegi max. deger = %d\n\n", SCHAR_MAX);

    printf("SHORT INT icin alabilecegi min. deger = %d\n", SHRT_MIN);
    printf("SHORT INT icin alabilecegi max. deger = %d\n\n", SHRT_MAX);

    printf("INT icin alabilecegi min. deger = %d\n", INT_MIN);
    printf("INT icin alabilecegi max. deger  = %d\n\n", INT_MAX);

    printf("CHAR icin alabilecegi min. deger = %d\n", CHAR_MIN);
    printf("CHAR icin alabilecegi max. deger  = %d\n\n", CHAR_MAX);

    printf("LONG INT icin alabilecegi min. deger = %ld\n", LONG_MIN);
    printf("LONG INT icin alabilecegi max. deger = %ld\n", LONG_MAX);

    return 0;
}
