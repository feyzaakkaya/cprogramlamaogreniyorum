#include <stdio.h>

int main(int argc, char**argv){
    int paraMiktari;

    printf("Lutfen para miktarini giriniz;\t");
    scanf("%d",&paraMiktari);

    printf("%d adet 200TL banknotu bulunmaktadir. \n",paraMiktari/200);
    paraMiktari%=200;
    printf("%d adet 100TL banknotu bulunmaktadir. \n",paraMiktari/100);
    paraMiktari%=100;
    printf("%d adet 50TL banknotu bulunmaktadir. \n",paraMiktari/50);
    paraMiktari%=50;
    printf("%d adet 20TL banknotu bulunmaktadir. \n",paraMiktari/20);
    paraMiktari%=20;
    printf("%d adet 10TL banknotu bulunmaktadir. \n",paraMiktari/10);
    paraMiktari%=10;
    printf("%d adet 5TL banknotu bulunmaktadir. \n",paraMiktari/5);
    paraMiktari%=5;
    printf("%d adet 1TL banknotu bulunmaktadir. \n",paraMiktari);

    return 0;
}
